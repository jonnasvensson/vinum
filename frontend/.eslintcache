div>\n              <input\n                className=\"input\"\n                name=\"username\"\n                type=\"text\"\n                value={login.username}\n                onChange={handleChange}\n              />\n            </div>\n            <div className=\"titleAndInputGroup\">\n              <div className=\"title\">Password</div>\n              <input\n                className=\"input\"\n                name=\"password\"\n                type=\"password\"\n                value={login.password}\n                onChange={handleChange}\n              />\n            </div>\n            <div className=\"buttonContainer\">\n              <input type=\"submit\" value=\"Sign in\" className=\"button\" />\n            </div>\n          </form>\n          <div className=\"registerContainer\">\n            <div>Register?</div>\n          </div>\n        </div>\n        : <Redirect to=\"/home\" />}\n        {/* <Route path=\"/login\" component={() => (\n          <Login\n            token={token}\n            setToken={setToken}\n          />\n        )}\n        /> */}\n        <Route exact path=\"/home\" component={() => (\n          <Home\n            token={token}\n          />\n        )}\n        />\n        <Route path=\"/add\" component={() => (\n          <Add\n            token={token}\n          />\n        )}\n        />\n        <Route path=\"/edit/:id\" component={() => (\n          <Edit\n            token={token}\n          />\n        )}\n        />\n      </Router>\n    </div>\n  );\n}\n\nexport default App;",["52","53"],"/home/node/app/src/components/Login.js",["54","55","56"],"/home/node/app/src/components/Home.js",["57"],"/home/node/app/src/components/Header.js",[],"/home/node/app/src/components/Add.js",["58","59"],"import '../App.scss';\nimport axios from 'axios';\nimport React, { useState, useEffect } from 'react';\nimport Header from './Header'\nimport AriaModal from 'react-aria-modal';\nimport CloseIcon from '@material-ui/icon